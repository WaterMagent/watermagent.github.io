#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/Administrator/valaxy-blog/node_modules/.pnpm/vite-ssg@25.2.0_beasties@0._12f61ca7920975ac99cba131848b20f4/node_modules/vite-ssg/bin/node_modules:/mnt/c/Users/Administrator/valaxy-blog/node_modules/.pnpm/vite-ssg@25.2.0_beasties@0._12f61ca7920975ac99cba131848b20f4/node_modules/vite-ssg/node_modules:/mnt/c/Users/Administrator/valaxy-blog/node_modules/.pnpm/vite-ssg@25.2.0_beasties@0._12f61ca7920975ac99cba131848b20f4/node_modules:/mnt/c/Users/Administrator/valaxy-blog/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/Administrator/valaxy-blog/node_modules/.pnpm/vite-ssg@25.2.0_beasties@0._12f61ca7920975ac99cba131848b20f4/node_modules/vite-ssg/bin/node_modules:/mnt/c/Users/Administrator/valaxy-blog/node_modules/.pnpm/vite-ssg@25.2.0_beasties@0._12f61ca7920975ac99cba131848b20f4/node_modules/vite-ssg/node_modules:/mnt/c/Users/Administrator/valaxy-blog/node_modules/.pnpm/vite-ssg@25.2.0_beasties@0._12f61ca7920975ac99cba131848b20f4/node_modules:/mnt/c/Users/Administrator/valaxy-blog/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../vite-ssg/bin/vite-ssg.js" "$@"
else
  exec node  "$basedir/../vite-ssg/bin/vite-ssg.js" "$@"
fi
