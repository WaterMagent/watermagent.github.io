#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/Administrator/valaxy-blog/node_modules/.pnpm/valaxy@0.22.16_@babel+parse_e5f20ff59a0b8124f26b2a98e59d34c2/node_modules/valaxy/bin/node_modules:/mnt/c/Users/Administrator/valaxy-blog/node_modules/.pnpm/valaxy@0.22.16_@babel+parse_e5f20ff59a0b8124f26b2a98e59d34c2/node_modules/valaxy/node_modules:/mnt/c/Users/Administrator/valaxy-blog/node_modules/.pnpm/valaxy@0.22.16_@babel+parse_e5f20ff59a0b8124f26b2a98e59d34c2/node_modules:/mnt/c/Users/Administrator/valaxy-blog/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/Administrator/valaxy-blog/node_modules/.pnpm/valaxy@0.22.16_@babel+parse_e5f20ff59a0b8124f26b2a98e59d34c2/node_modules/valaxy/bin/node_modules:/mnt/c/Users/Administrator/valaxy-blog/node_modules/.pnpm/valaxy@0.22.16_@babel+parse_e5f20ff59a0b8124f26b2a98e59d34c2/node_modules/valaxy/node_modules:/mnt/c/Users/Administrator/valaxy-blog/node_modules/.pnpm/valaxy@0.22.16_@babel+parse_e5f20ff59a0b8124f26b2a98e59d34c2/node_modules:/mnt/c/Users/Administrator/valaxy-blog/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../valaxy@0.22.16_@babel+parse_e5f20ff59a0b8124f26b2a98e59d34c2/node_modules/valaxy/bin/valaxy.js" "$@"
else
  exec node  "$basedir/../../../../../../valaxy@0.22.16_@babel+parse_e5f20ff59a0b8124f26b2a98e59d34c2/node_modules/valaxy/bin/valaxy.js" "$@"
fi
